<div class="container">
	<div class="row">
		<div class="col-12">
			<h4>ユーザー一覧</h4>
			<table class="table table-bordered">
				<thead>
					<tr>
						<th>ID</th>
						<th>名前</th>
						<th>登録日</th>
					</tr>
				</thead>
				<tbody>
					<% @users.each do |user| %>
						<tr>
							<td><%= user.id %></td>
							<td><%= link_to user.name, admins_user_path(user) %></td>
							<td><%= user.created_at %></td>
						</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
	<%= paginate @users %>
</div>